  "bugs": {
    "url": "https://github.com/EagerApps/PayPalButtons/issues"
  },
  "homepage": "https://github.com/EagerApps/PayPalButtons#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.9.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "onchange": "^2.4.0",
    "postcss-cli": "2.5.2",
    "rollup": "0.26.2",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-includepaths": "0.1.5",
    "rollup-plugin-node-resolve": "1.5.0",
    "rollup-plugin-string": "^1.0.1",
    "watch": "0.18.0"
  }
